{
  "name": "@quintype/build",
  "version": "3.7.0",
  "description": "Library that defines build process for node apps",
  "main": "src/index.js",
  "scripts": {
    "lint:js:ci": "npx eslint $(git diff --diff-filter=ACM --pretty='' --name-only origin/master..HEAD -- '*.js') 'src/index.js'",
    "prepublishOnly": "./bin-dev-scripts/standard-version-release.sh",
    "sync-files-to": "npx onchange --verbose --wait --await-write-finish '**/*' -- ./bin-dev-scripts/sync-to.sh "
  },
  "keywords": [
    "node",
    "webpack",
    "quintype"
  ],
  "bin": {
    "quintype-build": "./bin/quintype-build",
    "quintype-app-build": "./bin/quintype-app-build"
  },
  "author": "Quintype Developers <dev-core@quintype.com>",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "autoprefixer": "^8.6.5",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-react-css-modules": "^3.4.2",
    "babel-plugin-transform-assets-import-to-string": "^1.2.0",
    "css-loader": "^2.1.0",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.11",
    "immer": "^1.12.1",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.7.2",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "path": "^0.12.7",
    "postcss": "^6.0.23",
    "postcss-loader": "^2.1.6",
    "precss": "^4.0.0",
    "process": "^0.11.10",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.23.1",
    "webpack": "^4.21.0 <4.29.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^2.0.13",
    "webpack-common-shake": "^2.1.0",
    "webpack-dev-server": "^3.1.14",
    "webpack-manifest-plugin": "^2.0.4",
    "yargs": "^12.0.5"
  },
  "devDependencies": {
    "eslint": "^5.8.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.1.3",
    "lint-staged": "^8.0.4",
    "onchange": "^4.1.0",
    "prettier": "1.14.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npx eslint --fix",
      "git add"
    ]
  }
}
