{
  "name": "@quintype/build",
  "version": "2.8.0",
  "description": "Library that defines build process for node apps",
  "main": "index.js",
  "scripts": {
    "lint:js:ci": "npx eslint $(git diff --pretty='' --name-only origin/master..HEAD -- '*.js') ./index.js",
    "prepublishOnly": "npm install && git diff --quiet && git tag -m '' -a v$(cat package.json | grep version | head -n1  | cut -d \\\" -f 4) && git push --follow-tags origin",
    "sync-files-to": "npx onchange --verbose --wait --await-write-finish '**/*' -- ./bin-dev-scripts/sync-to.sh "
  },
  "keywords": [
    "node",
    "webpack",
    "quintype"
  ],
  "author": "Quintype Developers <dev-core@quintype.com>",
  "license": "ISC",
  "dependencies": {
    "babel-plugin-quintype-assets": "^1.0.2",
    "babel-plugin-react-css-modules": "^3.3.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-register": "^6.26.0",
    "mini-css-extract-plugin": "^0.4.0",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "path": "^0.12.7",
    "process": "^0.11.10",
    "webpack": "^4.4.1",
    "webpack-manifest-plugin": "^2.0.0-rc.2"
  },
  "peerDependencies": {
    "autoprefixer": "^8.2.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-es2015-tree-shaking": "^1.0.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.1.3",
    "precss": "^3.1.2",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.19.0",
    "webpack": "^4.4.1"
  },
  "bin": {
    "quintype-build": "./bin/quintype-build"
  },
  "devDependencies": {
    "eslint": "^5.8.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.1.3",
    "lint-staged": "^8.0.4",
    "onchange": "^4.1.0",
    "prettier": "1.14.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npx eslint --fix",
      "git add"
    ]
  }
}
