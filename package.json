{
  "name": "@quintype/build",
  "version": "3.11.0-webpack5.9",
  "description": "Library that defines build process for node apps",
  "main": "src/index.js",
  "scripts": {
    "docs": "rimraf docs && jsdoc -c jsdoc.json",
    "lint:js:ci": "npx eslint $(git diff --diff-filter=ACM --pretty='' --name-only origin/master..HEAD -- '*.js') 'src/index.js'",
    "prepublishOnly": "./bin-dev-scripts/standard-version-release.sh",
    "sync-files-to": "npx onchange --verbose --wait --await-write-finish '**/*' -- ./bin-dev-scripts/sync-to.sh "
  },
  "keywords": [
    "node",
    "webpack",
    "quintype"
  ],
  "bin": {
    "quintype-build": "./bin/quintype-build"
  },
  "author": "Quintype Developers <dev-core@quintype.com>",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@babel/register": "^7.13.8",
    "@babel/runtime": "^7.13.10",
    "autoprefixer": "^9.8.6",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-react-css-modules": "^5.2.6",
    "babel-plugin-transform-assets-import-to-string": "^1.2.0",
    "css-loader": "^5.1.3",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "file-loader": "^6.2.0",
    "immer": "^8.0.1",
    "lodash": "^4.17.21",
    "lodash-webpack-plugin": "^0.11.6",
    "mini-css-extract-plugin": "^1.3.9",
    "node-sass": "^5.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "path": "^0.12.7",
    "postcss": "^7.0.35",
    "postcss-loader": "^4.2.0",
    "precss": "^4.0.0",
    "process": "^0.11.10",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "webpack": "^5.26.2",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
    "webpack-common-shake": "^2.1.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-manifest-plugin": "^3.1.0",
    "webpack-nano": "^1.1.1",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "better-docs": "^2.3.2",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^4.1.0",
    "gh-pages": "^3.1.0",
    "husky": "^5.1.3",
    "jsdoc": "^3.6.6",
    "lint-staged": "^10.5.4",
    "onchange": "^4.1.0",
    "prettier": "2.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npx eslint --fix",
      "git add"
    ]
  }
}
