{
  "name": "@quintype/build",
  "version": "3.8.1",
  "description": "Library that defines build process for node apps",
  "main": "src/index.js",
  "scripts": {
    "lint:js:ci": "npx eslint $(git diff --diff-filter=ACM --pretty='' --name-only origin/master..HEAD -- '*.js') 'src/index.js'",
    "prepublishOnly": "./bin-dev-scripts/standard-version-release.sh",
    "sync-files-to": "npx onchange --verbose --wait --await-write-finish '**/*' -- ./bin-dev-scripts/sync-to.sh "
  },
  "keywords": [
    "node",
    "webpack",
    "quintype"
  ],
  "bin": {
    "quintype-build": "./bin/quintype-build"
  },
  "author": "Quintype Developers <dev-core@quintype.com>",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.3.1",
    "autoprefixer": "^9.4.8",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-react-css-modules": "^5.2.1",
    "babel-plugin-transform-assets-import-to-string": "^1.2.0",
    "css-loader": "^2.1.0",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "file-loader": "^3.0.1",
    "immer": "^2.0.0",
    "lodash": "^4.17.11",
    "lodash-webpack-plugin": "^0.11.5",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "path": "^0.12.7",
    "postcss": "^7.0.14",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "process": "^0.11.10",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.5",
    "webpack-bundle-analyzer": "^3.0.4",
    "webpack-cli": "^3.2.3",
    "webpack-common-shake": "^2.1.0",
    "webpack-dev-server": "^3.2.0",
    "webpack-manifest-plugin": "^2.0.4",
    "yargs": "^13.2.1"
  },
  "devDependencies": {
    "eslint": "^5.14.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.4",
    "onchange": "^4.1.0",
    "prettier": "1.16.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npx eslint --fix",
      "git add"
    ]
  }
}
